    mixin tableLineBasic(parametr, value, status, statusValue, request)
        tr.table__line
            td.table__item.table__item_body= parametr
            td.table__item.table__item_body= value
            td.table__item.table__item_body
                if request
                    .status.status_minimal.status_warning(data-type=status data-toggle="tooltip" data-placement="top" title="new request")= statusValue

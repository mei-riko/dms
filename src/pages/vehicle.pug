                            path(d="M7.35385 4H8.65769L8.43846 9.42776H7.56154L7.35385 4ZM8.01154 12C7.79615 12 7.61538 11.932 7.46923 11.796C7.32308 11.6525 7.25 11.4788 7.25 11.2748C7.25 11.0708 7.32308 10.9009 7.46923 10.7649C7.61538 10.6213 7.79615 10.5496 8.01154 10.5496C8.22692 10.5496 8.40385 10.6213 8.54231 10.7649C8.68077 10.9009 8.75 11.0708 8.75 11.2748C8.75 11.4788 8.67692 11.6525 8.53077 11.796C8.39231 11.932 8.21923 12 8.01154 12Z")
                            path(d="M4 1H12V-1H4V1ZM15 4V12H17V4H15ZM12 15H4V17H12V15ZM1 12V4H-1V12H1ZM4 15C2.34315 15 1 13.6569 1 12H-1C-1 14.7614 1.23858 17 4 17V15ZM15 12C15 13.6569 13.6569 15 12 15V17C14.7614 17 17 14.7614 17 12H15ZM12 1C13.6569 1 15 2.34315 15 4H17C17 1.23858 14.7614 -1 12 -1V1ZM4 -1C1.23858 -1 -1 1.23858 -1 4H1C1 2.34315 2.34315 1 4 1V-1Z")
                else
                    .status.status_minimal(data-type=status)= statusValue
            td.table__item.table__item_body.table__item_action-body
                select.select.select_table(data-placeholder="Action")
                    option 
                    option(value="1" data-action="#changeParamModal" data-type="modal") Change
                    option(value="2" data-action="#requestParamModal" data-type="modal") Add request
                    if request
                        option(value="3" data-action="#requestViewModal" data-type="modal") View request
    mixin tableLineBasicSmall(parametr, value, status)
        tr.table__line
            td.table__item.table__item_body= parametr
            td.table__item.table__item_body= value
            td.table__item.table__item_body= status
            td.table__item.table__item_body.table__item_action-body
                select.select.select_table(data-placeholder="Action")
                    option 
                    option(value="1" data-action="" data-type="") Change
                    option(value="2" data-action="" data-type="") Full information
    mixin tableLineDevice(device, model, serial, firmware, firmwareStatus, status)
        tr.table__line
            td.table__item.table__item_body= device
            td.table__item.table__item_body= model
            td.table__item.table__item_body= serial
            td.table__item.table__item_body= firmware
            td.table__item.table__item_body
                .status.status_minimal(data-type=status)= firmwareStatus
            td.table__item.table__item_body.table__item_action-body
                select.select.select_table(data-placeholder="Action")
                    option 
                    option(value="2" data-action="vehicle-display.html" data-type="link") Enter
                    option(value="1" data-action="" data-type="") Change
    mixin tableLineConnect(parametr, connection, data)
        tr.table__line
            td.table__item.table__item_body= parametr
            td.table__item.table__item_body
                if connection
                    .status.status_label(data-type="true") On
                else
                    .status.status_label(data-type="false") Off
            td.table__item.table__item_body= data
            td.table__item.table__item_body.text-end
                a.btn.btn-secondary.btn--small(href="#") Refresh
